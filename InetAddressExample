import java.net.InetAddress;

public class InetAddressExample {
    public static void main(String[] args) {
        try {
            InetAddress address = InetAddress.getByName("www.google.com");
            System.out.println("Hostname: " + address.getHostName());
            System.out.println("Ip Address :" + address.getHostAddress());


            InetAddress address2 = InetAddress.getByName("www.instagram.com");
            System.out.println("Hostname: " + address2.getHostName());
            System.out.println("Ip Address :" + address2.getHostAddress());

            InetAddress address3 = InetAddress.getByName("www.worldlink.com");

            System.out.println("Hostname: " + address3.getHostName());
            System.out.println("Ip Address :" + address3.getHostAddress());

        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
